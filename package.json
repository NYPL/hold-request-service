{
  "name": "HoldRequestService",
  "version": "0.1.0",
  "description": "Lambda for the NYPL Hold Request API Service",
  "main": "index.js",
  "scripts": {
    "pretest": "phplint src/*.php src/**/*.php src/**/**/*.php",
    "test-request": "node-lambda run -j config/event_sources_development.json -f tests/Mocks/mock.env",
    "deploy-development": "node-lambda deploy -e development -f config/var_development.env -S config/event_sources_development.json -b {subnets} -g {securitygroups}",
    "deploy-qa": "./node_modules/.bin/node-lambda deploy -e qa -f config/var_qa.env -S config/event_sources_qa.json --role --profile -b {subnets} -g {securitygroups}",
    "deploy-production": "node-lambda deploy -e production -f config/var_production.env -S config/event_sources_production.json -b {subnets} -g {securitygroups}"
  },
  "repository": {},
  "keywords": [],
  "dependencies": {
    "http-string-parser": "^0.0.5",
    "node-lambda": "^0.12.0"
  },
  "devDependencies": {
    "phplint": "~1.0.0"
  }
}
