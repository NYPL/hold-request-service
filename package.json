{
  "name": "HoldRequestService",
  "version": "0.1.0",
  "description": "Lambda for the NYPL Hold Request API Service",
  "main": "index.js",
  "scripts": {
    "pretest": "phplint src/*.php src/**/*.php src/**/**/*.php",
    "test-request": "node-lambda run -j config/event_sources_development.json -f config/var_testing.env",
    "deploy-development": "node-lambda deploy -e development -f config/var_development.env -S config/event_sources_development.json -b subnet-f4fe56af -g sg-1d544067 -P nypl-sandbox",
    "deploy-qa": "node-lambda deploy -e qa -f config/var_qa.env -S config/event_sources_qa.json -b {subnets} -g {securitygroups}",
    "deploy-production": "node-lambda deploy -e production -f config/var_production.env -S config/event_sources_production.json -b {subnets} -g {securitygroups}"
  },
  "repository": {},
  "keywords": [],
  "dependencies": {
    "http-string-parser": "^0.0.5"
  },
  "devDependencies": {
    "phplint": "~1.0.0"
  }
}
